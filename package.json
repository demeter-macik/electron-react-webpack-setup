{
  "name": "electron-react-webpack-setup",
  "version": "0.2.0",
  "description": "electron react webpack setup",
  "main": "index.js",
  "scripts": {
    "start": "npm run build; node ./node_modules/.bin/electron .",
    "build": "node ./node_modules/.bin/webpack --config webpack.dev.js",
    "watch": "node ./node_modules/.bin/webpack --config webpack.dev.js --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demeter-macik/electron-react-webpack-setup.git"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "electron": "^3.0.6",
    "electron-reload": "^1.2.5",
    "eslint": "^5.8.0",
    "eslint-plugin-react": "^7.11.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "author": "Demeter Macik",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/demeter-macik/electron-react-webpack-setup/issues"
  },
  "homepage": "https://github.com/demeter-macik/electron-react-webpack-setup#readme",
  "dependencies": {
    "material-ui": "^0.20.2"
  }
}
